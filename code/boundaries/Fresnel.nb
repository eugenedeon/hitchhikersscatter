Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"refract", "[", 
    RowBox[{"w_", ",", "n_", ",", "eta1_", ",", "eta2_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{"-", "eta1"}], "eta2"], 
     RowBox[{"(", 
      RowBox[{"w", "-", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"w", ".", "n"}], ")"}], "n"}]}], ")"}]}], "-", 
    RowBox[{
     RowBox[{"(", 
      SqrtBox[
       RowBox[{"1", "-", 
        RowBox[{
         SuperscriptBox[
          RowBox[{"(", 
           FractionBox["eta1", "eta2"], ")"}], "2"], 
         RowBox[{"(", 
          RowBox[{"1", "-", 
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"w", ".", "n"}], ")"}], "2"]}], ")"}]}]}]], ")"}], 
     "n"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"reflect", "[", 
    RowBox[{"v_", ",", "n_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"-", "v"}], "+", 
    RowBox[{"2", " ", "n", " ", 
     RowBox[{"n", ".", "v"}]}]}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.604104693928533*^9, 3.604104696512973*^9}},
