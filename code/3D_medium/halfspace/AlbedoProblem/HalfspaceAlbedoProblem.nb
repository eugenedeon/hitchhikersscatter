          FractionBox[
           RowBox[{
            RowBox[{"(", 
             RowBox[{"24", "-", 
              RowBox[{"12", " ", "c"}], "+", 
              SuperscriptBox["c", "2"]}], ")"}], " ", 
            SuperscriptBox["\[ExponentialE]", 
             RowBox[{
              RowBox[{"-", "4"}], "/", "c"}]]}], 
           SuperscriptBox["c", "2"]], "+", 
          FractionBox[
           RowBox[{
            RowBox[{"(", 
             RowBox[{"4", "-", "c"}], ")"}], " ", 
            SuperscriptBox["\[ExponentialE]", 
             RowBox[{
              RowBox[{"-", "2"}], "/", "c"}]]}], "c"]}], ")"}]}]}], ")"}]}], 
    ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"FindRoot", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"c", " ", "v", " ", 
           RowBox[{"ArcTanh", "[", 
