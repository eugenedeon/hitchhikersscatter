            RowBox[{"-", "14"}]]}], ",", 
          SuperscriptBox["10", "14"]}], "}"}], ",", 
        RowBox[{"Method", "\[Rule]", "\"\<Brent\>\""}]}], "]"}], "[", 
      RowBox[{"[", "1", "]"}], "]"}], "[", 
