Cell["\<\
Sampling strategy using 4 uniform random variates from [Mikhailov 1966, \
Marchuk and Mikhailov 1967]:\
\>", "Text",
 CellChangeTimes->{{3.8305820710596952`*^9, 3.830582086995901*^9}, {
  3.830583553978816*^9, 
  3.830583557302466*^9}},ExpressionUUID->"18520669-209f-4960-b30f-\
bdaaa73d1131"],

Cell[BoxData[
 RowBox[{
  RowBox[{"sampleMikhailov1966", "[", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"a", ",", "a1", ",", "a2", ",", "a3"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{"a", ",", "a1", ",", "a2", ",", "a3"}], "}"}], "=", 
      RowBox[{"RandomReal", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"0", ",", "1"}], "}"}], ",", "4"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"0", "\[LessEqual]", "a", "\[LessEqual]", 
        FractionBox["3", "4"]}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         FractionBox["8", "3"], "a"}], "-", "1"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Sign", "[", 
         RowBox[{"a", "-", 
          FractionBox["7", "8"]}], "]"}], 
        RowBox[{"Max", "[", 
         RowBox[{"a1", ",", "a2", ",", "a3"}], "]"}]}]}], 
      "\[IndentingNewLine]", "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.8305835683757343`*^9, 3.8305835757118893`*^9}},
 CellLabel->
  "In[7317]:=",ExpressionUUID->"8d334165-4c59-4c9b-bc1f-0b9179cd5c99"],

