  "In[240]:=",ExpressionUUID->"17d9c738-6bf5-4e33-add2-438248b05e73"],

Cell["\<\
project down onto a 3D sphere, build a coordinate frame, and sample a white \
Lambertian BRDF\
\>", "Text",
 CellChangeTimes->{{3.774809290115534*^9, 
  3.774809305819785*^9}},ExpressionUUID->"fafab677-af16-46c8-be1e-\
53f722313fc4"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"lambertspheresample", "[", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"p", ",", "Z", ",", "X", ",", "Y", ",", "w", ",", "s"}], "}"}], 
     ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"p", "=", 
       RowBox[{"DiskSample3D", "[", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"p", "[", 
        RowBox[{"[", "3", "]"}], "]"}], "=", 
       SqrtBox[
        RowBox[{"1", "-", 
         SuperscriptBox[
          RowBox[{"p", "[", 
           RowBox[{"[", "1", "]"}], "]"}], "2"], "-", 
         SuperscriptBox[
          RowBox[{"p", "[", 
           RowBox[{"[", "2", "]"}], "]"}], "2"]}]]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
        RowBox[{"building", " ", "basis"}], ",", " ", "X", ",", "Y", ",", 
        "Z"}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"Z", "=", "p"}], ";", "\[IndentingNewLine]", 
      RowBox[{"X", "=", 
       RowBox[{"Normalize", "[", 
        RowBox[{"Cross", "[", 
         RowBox[{"Z", ",", 
          RowBox[{"{", 
           RowBox[{"1", ",", "0", ",", "0"}], "}"}]}], "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Y", "=", 
       RowBox[{"Cross", "[", 
        RowBox[{"X", ",", "Z"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{"sample", " ", "Lambertian", " ", "reflectance"}], " ", "*)"}],
       "\[IndentingNewLine]", 
      RowBox[{"w", "=", 
       SqrtBox[
        RowBox[{"RandomReal", "[", "]"}]]}], ";", "\[IndentingNewLine]", 
      RowBox[{"p", "=", 
       RowBox[{"RandomReal", "[", 
        RowBox[{"{", 
         RowBox[{"0", ",", 
          RowBox[{"2", "Pi"}]}], "}"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"s", "=", 
       SqrtBox[
        RowBox[{"1", "-", 
         SuperscriptBox["w", "2"]}]]}], ";", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
       "rotate", " ", "into", " ", "space", " ", "of", " ", "selected", " ", 
        "sphere", " ", "normal", " ", "p"}], " ", "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"-", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"s", " ", 
          RowBox[{"Cos", "[", "p", "]"}], " ", "X"}], " ", "+", " ", 
         RowBox[{"s", " ", 
          RowBox[{"Sin", "[", "p", "]"}], " ", "Y"}], " ", "+", " ", 
         RowBox[{"w", " ", "Z"}]}], ")"}]}]}]}], "\[IndentingNewLine]", 
    "]"}]}], "\[IndentingNewLine]"}]], "Input",
 CellChangeTimes->{{3.774808566952692*^9, 3.774808594230905*^9}, {
  3.774808655295662*^9, 3.774808664470406*^9}, {3.774809191867906*^9, 
