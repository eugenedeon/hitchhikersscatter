Cell[BoxData[
 RowBox[{
  RowBox[{"DiskSample3D", "[", "]"}], ":=", 
  RowBox[{
   RowBox[{
    RowBox[{
     SqrtBox[
      RowBox[{"RandomReal", "[", "]"}]], 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Cos", "[", "#", "]"}], ",", 
       RowBox[{"Sin", "[", "#", "]"}], ",", "0"}], "}"}]}], "&"}], "[", 
   RowBox[{
    RowBox[{"RandomReal", "[", "]"}], " ", "2", " ", "Pi"}], "]"}]}]], "Input",\

