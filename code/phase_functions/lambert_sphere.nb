Cell[BoxData[
 RowBox[{
  RowBox[{"pLambertSphere", "[", "u_", "]"}], ":=", 
  FractionBox[
   RowBox[{"2", " ", 
    RowBox[{"(", 
     RowBox[{
      SqrtBox[
       RowBox[{"1", "-", 
        SuperscriptBox["u", "2"]}]], "-", 
      RowBox[{"u", " ", 
       RowBox[{"ArcCos", "[", "u", "]"}]}]}], ")"}]}], 
   RowBox[{"3", " ", 
    SuperscriptBox["\[Pi]", "2"]}]]}]], "Input",
 CellChangeTimes->{{3.774809236755698*^9, 3.774809245986871*^9}, {
   3.774809440016292*^9, 3.7748094415733833`*^9}, 3.774810121864068*^9},
