Cell[CellGroupData[{

Cell[TextData[{
 "When the inverse Laplace transform ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubsuperscriptBox["\[ScriptCapitalL]", "r", 
      RowBox[{"-", "1"}]], "[", 
     RowBox[{"f", "(", 
      SqrtBox["r"], ")"}], "]"}], 
    RowBox[{"(", "s", ")"}]}], TraditionalForm]],
  CellChangeTimes->{{3.79503435556837*^9, 3.795034381582061*^9}},
  ExpressionUUID->"bc34ff36-f6db-4bf5-82b7-7386374200a8"],
 " is known (superpositions of Gaussians)"
}], "Subsection",
 CellChangeTimes->{{3.7950343881403723`*^9, 
  3.7950344092007017`*^9}},ExpressionUUID->"4d3e3b95-2f2c-4165-80a1-\
5574214fedd2"],

Cell[TextData[{
 "Sometimes the inverse Laplace transform of f(r) is not known but the \
inverse Laplace transform of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"f", "(", 
      SqrtBox["r"], ")"}], " ", 
     RowBox[{"is", ".", "  ", "In"}], " ", "this", " ", "case"}], ",", " ", 
    RowBox[{
    "it", " ", "may", " ", "be", " ", "possible", " ", "to", " ", "sample", 
     " "}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "076ea309-baec-499d-9ac1-d36f5305d0d1"],
 "f(r) using the Gaussian (or Normal) distribution.  To do this, use the \
Gaussian transform, described next."
}], "Text",
 CellChangeTimes->{{3.7950349411685257`*^9, 3.79503499709984*^9}, {
  3.7950353335789137`*^9, 
  3.795035340786779*^9}},ExpressionUUID->"2edc843e-933d-43f5-83ea-\
cff7c4c55e1b"]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

Cell["6. The Gaussian Transform", "Section",
 CellChangeTimes->{{3.795034528093306*^9, 3.795034530433448*^9}, {
  3.795045525966815*^9, 
  3.795045526222563*^9}},ExpressionUUID->"0b8c3adb-5b12-4e97-90a6-\
74766fdc2c72"],

Cell["\<\
The 1D Gaussian/Normal distribution on the half line with variance s is:\
\>", "Text",
 CellChangeTimes->{{3.7950357209341793`*^9, 
  3.795035734092602*^9}},ExpressionUUID->"f384595e-448f-4528-9977-\
9b59ba6b7b51"],

Cell[BoxData[
 RowBox[{
  RowBox[{"Gaussian", "[", 
   RowBox[{"r_", ",", "s_"}], "]"}], ":=", 
  RowBox[{
   FractionBox["2", 
    SqrtBox[
     RowBox[{"2", " ", "Pi", " ", "s"}]]], 
   RowBox[{"Exp", "[", 
    RowBox[{
     RowBox[{"-", 
      SuperscriptBox["r", "2"]}], "/", 
     RowBox[{"(", 
      RowBox[{"2", " ", "s"}], ")"}]}], "]"}]}]}]], "Input",
 CellChangeTimes->{{3.795035709221348*^9, 3.7950357119974737`*^9}, {
  3.795035793822281*^9, 3.7950357939423847`*^9}, {3.7950359337279043`*^9, 
  3.7950359373686028`*^9}},
 CellLabel->
  "In[1603]:=",ExpressionUUID->"2cb55527-768c-4720-9875-ecf6921b4a48"],

Cell["and is normalized on the half line:", "Text",
 CellChangeTimes->{{3.795035737841434*^9, 
  3.7950357413075047`*^9}},ExpressionUUID->"4a93f87c-1ca5-4689-9466-\
3ef5a8b480ea"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Integrate", "[", 
  RowBox[{
   RowBox[{"Gaussian", "[", 
    RowBox[{"r", ",", "s"}], "]"}], ",", 
   RowBox[{"{", 
    RowBox[{"r", ",", "0", ",", "Infinity"}], "}"}], ",", 
   RowBox[{"Assumptions", "\[Rule]", 
    RowBox[{"s", ">", "0"}]}]}], "]"}]], "Input",
 CellChangeTimes->{{3.795035742718852*^9, 3.795035765796768*^9}, {
  3.795035944863154*^9, 3.79503594644405*^9}},
 CellLabel->
  "In[1604]:=",ExpressionUUID->"2f1883e4-8140-46f1-aad0-e9a4e33a1ed8"],

Cell[BoxData["1"], "Output",
 CellChangeTimes->{{3.795035752803316*^9, 3.795035795810705*^9}, 
   3.795035946996984*^9},
 CellLabel->
  "Out[1604]=",ExpressionUUID->"a5522119-26af-4de1-b4be-405e61e20d39"]
}, Open  ]],

Cell["\<\
This one-sided Gaussian/Normal distribution can be sampled using inverseErf[] \
or the Box-Muller algorithm.\
\>", "Text",
 CellChangeTimes->{{3.7950361173136663`*^9, 
  3.795036145283038*^9}},ExpressionUUID->"af59c914-6aad-4a35-95ba-\
1fa4fe99dfad"],

Cell[TextData[{
 "The Gaussian transform [Alecu et al. 2006] can be used to solve for a \
weight function w[s] such that f(r) can be expressed as a superposition of \
Gaussians: f(r) = ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
    RowBox[{
     RowBox[{"Gaussian", "(", 
      RowBox[{"r", ",", "s"}], ")"}], 
     RowBox[{"w", "(", "s", ")"}], 
     RowBox[{"\[DifferentialD]", "s"}]}]}], TraditionalForm]],ExpressionUUID->
  "9f348084-bea4-4404-8af9-e6924cb6aa34"],
 "."
}], "Text",
 CellChangeTimes->{{3.795035886731758*^9, 
  3.7950359580604477`*^9}},ExpressionUUID->"c147afd5-b2dc-4c5c-b595-\
159f2224581e"],

Cell[CellGroupData[{

Cell["Example 6.1 - w[s] is Erlang-2/Gamma-2:", "Subsection",
 CellChangeTimes->{{3.7950359894729967`*^9, 3.7950359981590223`*^9}, {
  3.795045528700575*^9, 
  3.7950455298553457`*^9}},ExpressionUUID->"e12e3b88-96a7-498c-aa27-\
476db98c7dbc"],

Cell[BoxData[
 RowBox[{
  RowBox[{"w", "[", "s_", "]"}], ":=", 
  RowBox[{
   RowBox[{"Exp", "[", 
    RowBox[{"-", "s"}], "]"}], "s"}]}]], "Input",
 CellChangeTimes->{{3.7950360016056623`*^9, 3.7950360104338913`*^9}},
 CellLabel->
  "In[1617]:=",ExpressionUUID->"c19138c1-cdb1-4261-a29c-ded0f62b555a"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Integrate", "[", 
  RowBox[{
   RowBox[{"w", "[", "s", "]"}], ",", 
   RowBox[{"{", 
    RowBox[{"s", ",", "0", ",", "Infinity"}], "}"}]}], "]"}]], "Input",
 CellLabel->
  "In[1621]:=",ExpressionUUID->"e79406d9-4824-430a-ae5a-979fa96d3e72"],

Cell[BoxData["1"], "Output",
 CellChangeTimes->{3.7950363902614822`*^9},
 CellLabel->
  "Out[1621]=",ExpressionUUID->"355ec0ea-9ae9-4ee4-a278-2057bbb6b144"]
}, Open  ]],

Cell["Find f(r) given w(s) as the weighting function of Gaussians:", "Text",
 CellChangeTimes->{{3.795036013374216*^9, 
  3.795036021602961*^9}},ExpressionUUID->"04667781-befa-43a6-a1fa-\
a48e41ca748e"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"f", "=", 
  RowBox[{"Integrate", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"w", "[", "s", "]"}], 
     RowBox[{"Gaussian", "[", 
      RowBox[{"r", ",", "s"}], "]"}]}], ",", 
    RowBox[{"{", 
     RowBox[{"s", ",", "0", ",", "Infinity"}], "}"}], ",", 
    RowBox[{"Assumptions", "\[Rule]", 
     RowBox[{"r", ">", "0"}]}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.79503602358064*^9, 3.795036055567321*^9}},
 CellLabel->
  "In[1608]:=",ExpressionUUID->"7247e141-662b-483b-9634-f307ae795b29"],

Cell[BoxData[
 FractionBox[
  RowBox[{
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{
     RowBox[{"-", 
      SqrtBox["2"]}], " ", "r"}]], " ", 
   RowBox[{"(", 
    RowBox[{"1", "+", 
     RowBox[{
      SqrtBox["2"], " ", "r"}]}], ")"}]}], 
  SqrtBox["2"]]], "Output",
 CellChangeTimes->{{3.7950360348424683`*^9, 3.795036056631308*^9}},
 CellLabel->
  "Out[1608]=",ExpressionUUID->"7b0041bc-7ab0-423d-904b-a8a97d217d56"]
}, Open  ]],

Cell["Try the Gaussian transform to find w(s) given f(r):", "Text",
 CellChangeTimes->{{3.795036061092381*^9, 3.795036066676155*^9}, {
  3.79503626370393*^9, 
  3.795036269440064*^9}},ExpressionUUID->"34ef429c-4151-40bc-a18e-\
e7c42c49e59f"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{
   FractionBox["1", 
    RowBox[{"2", " ", "s"}]], 
   SqrtBox[
    FractionBox["Pi", 
     RowBox[{"2", "s"}]]], 
   RowBox[{"InverseLaplaceTransform", "[", 
    RowBox[{
     RowBox[{"f", "/.", 
      RowBox[{"r", "\[Rule]", 
       SqrtBox["r"]}]}], ",", "r", ",", "t"}], "]"}]}], "/.", 
  RowBox[{"t", "\[Rule]", 
   FractionBox["1", 
    RowBox[{"2", " ", "s"}]]}]}]], "Input",
 CellChangeTimes->{{3.795036075258157*^9, 3.795036095563574*^9}, {
  3.795036171991694*^9, 3.79503617232808*^9}},
 CellLabel->
  "In[1610]:=",ExpressionUUID->"c675c612-201b-40b9-a668-69b2b14ac473"],

Cell[BoxData[
 RowBox[{
  SuperscriptBox["\[ExponentialE]", 
   RowBox[{"-", "s"}]], " ", "s"}]], "Output",
 CellChangeTimes->{3.795036096355708*^9, 3.7950361729776163`*^9},
 CellLabel->
  "Out[1610]=",ExpressionUUID->"9763a8d7-5dde-4ecc-8472-8b83573f9aee"]
}, Open  ]]
}, Open  ]],

