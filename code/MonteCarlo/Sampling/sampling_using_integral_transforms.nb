Cell[BoxData[
 RowBox[{
  RowBox[{"f", "[", "r_", "]"}], ":=", 
  RowBox[{
   SqrtBox[
    FractionBox["2", "\[Pi]"]], "-", 
   RowBox[{
    SuperscriptBox["\[ExponentialE]", 
     FractionBox[
      SuperscriptBox["r", "2"], "2"]], " ", "r", " ", 
    RowBox[{"Erfc", "[", 
