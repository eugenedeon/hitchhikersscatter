Cell[BoxData[
 RowBox[{"1", "-", 
  FractionBox["1", 
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"1", "+", 
      SuperscriptBox["R", "2"]}], ")"}], 
    RowBox[{"3", "/", "2"}]]]}]], "Output",
 CellChangeTimes->{3.795071409024481*^9, 3.795071486447194*^9},
 CellLabel->
  "Out[1821]=",ExpressionUUID->"19a4a7f1-98ca-44df-8f08-d82b86018063"]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Solve", "[", 
  RowBox[{
   RowBox[{"xi", "==", 
    RowBox[{"1", "-", 
     FractionBox["1", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"1", "+", 
         SuperscriptBox["R", "2"]}], ")"}], 
       RowBox[{"3", "/", "2"}]]]}]}], ",", "R"}], "]"}]], "Input",
 CellChangeTimes->{{3.795071411427422*^9, 3.795071416043243*^9}, 
