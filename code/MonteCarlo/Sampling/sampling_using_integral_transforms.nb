Cell["\<\
We consider here an example that combines several of the above techniques to \
sample the distribution of visible normals for GGX by using the exact \
sampling method for Beckmann.  This is far less efficient than known sampling \
procedures for the GGX vNDF sampling [Heitz 2018].  However, the procedure \
highlights several of the above approaches.  Based on the observation that \
GGX is simply a superposition of Beckmanns:\
\>", "Text",
 CellChangeTimes->{{3.795052686038431*^9, 3.795052729738615*^9}, {
  3.795071617856106*^9, 
  3.795071673480775*^9}},ExpressionUUID->"6b9eea29-5be6-4acd-a3ae-\
089e99c2fd98"],

Cell[BoxData[
 RowBox[{
  RowBox[{"DBeckmann", "[", 
   RowBox[{"u_", ",", "\[Alpha]_"}], "]"}], ":=", 
  RowBox[{
   FractionBox["1", 
    RowBox[{"Pi", " ", 
     SuperscriptBox["\[Alpha]", "2"], " ", 
     SuperscriptBox[
      RowBox[{"(", "u", ")"}], "4"]}]], 
   SuperscriptBox["E", 
    RowBox[{"-", 
     RowBox[{"(", 
      FractionBox[
       RowBox[{"(", 
        RowBox[{
         FractionBox["1", 
          SuperscriptBox[
           RowBox[{"(", "u", ")"}], "2"]], "-", "1"}], ")"}], 
       SuperscriptBox["\[Alpha]", "2"]], ")"}]}]]}]}]], "Input",
 CellChangeTimes->{{3.771275869677575*^9, 3.771275871796411*^9}, {
  3.7712759558556337`*^9, 3.771275969774023*^9}},
 CellLabel->
  "In[1825]:=",ExpressionUUID->"bc33e08e-2fd0-423b-a8d2-db214aee88ce"],

Cell[BoxData[
 RowBox[{
  RowBox[{"DGGX", "[", 
   RowBox[{"u_", ",", "\[Alpha]_"}], "]"}], ":=", 
  FractionBox[
   RowBox[{
    SuperscriptBox["\[Alpha]", "2"], " ", 
    FractionBox["1", 
     SuperscriptBox[
      RowBox[{"(", "u", ")"}], "4"]]}], 
   RowBox[{"\[Pi]", " ", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["\[Alpha]", "2"], "+", 
       RowBox[{"(", 
        RowBox[{
         FractionBox["1", 
          SuperscriptBox[
           RowBox[{"(", "u", ")"}], "2"]], "-", "1"}], ")"}]}], ")"}], 
     "2"]}]]}]], "Input",
 CellLabel->
  "In[1826]:=",ExpressionUUID->"a10040f6-e376-4e9a-8bfc-59715ab4f730"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"Integrate", "[", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"2", " ", 
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{"-", 
          FractionBox[
           SuperscriptBox["GGXm", "2"], 
           SuperscriptBox["m", "2"]]}]], " ", 
        SuperscriptBox["GGXm", "2"]}], 
       SuperscriptBox["m", "3"]], 
      RowBox[{"DBeckmann", "[", 
       RowBox[{"u", ",", "m"}], "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"m", ",", "0", ",", "Infinity"}], "}"}], ",", 
     RowBox[{"Assumptions", "\[Rule]", 
      RowBox[{
       RowBox[{"GGXm", ">", "0"}], "&&", 
       RowBox[{"0", "<", "u", "<", "1"}]}]}]}], "]"}], "-", 
   RowBox[{"DGGX", "[", 
    RowBox[{"u", ",", "GGXm"}], "]"}]}], "//", "FullSimplify"}]], "Input",
