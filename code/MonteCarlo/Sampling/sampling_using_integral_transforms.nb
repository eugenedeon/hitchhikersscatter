ec22046eb861"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Integrate", "[", 
  RowBox[{
   RowBox[{"w", "[", "s", "]"}], ",", 
   RowBox[{"{", 
    RowBox[{"s", ",", "1", ",", "S"}], "}"}], ",", 
   RowBox[{"Assumptions", "\[Rule]", 
    RowBox[{"S", ">", "1"}]}]}], "]"}]], "Input",
 CellChangeTimes->{{3.7950384609025087`*^9, 3.795038486075193*^9}, {
  3.795043808704808*^9, 3.795043810751683*^9}},
