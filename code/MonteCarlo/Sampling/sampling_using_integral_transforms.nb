          RowBox[{
           RowBox[{
            FractionBox[
             RowBox[{
              RowBox[{"-", 
               SqrtBox["#"]}], "-", "#"}], 
             RowBox[{
              RowBox[{"-", "1"}], "+", "#"}]], "&"}], "[", 
           RowBox[{"RandomReal", "[", "]"}], "]"}], ")"}]]}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", 
        RowBox[{"Range", "[", "700", "]"}]}], "}"}]}], "]"}], "700"]}], 
  ";"}]], "Input",
