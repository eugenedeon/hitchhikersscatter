Cell[BoxData[
 RowBox[{
  RowBox[{"samplemprime", "[", "u_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "xi1", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"xi1", "=", 
      RowBox[{"RandomReal", "[", "]"}]}], ";", " ", 
     RowBox[{"(*", " ", 
      RowBox[{"term", " ", "selection", " ", "variable"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"xi1", "<", 
        FractionBox["u", 
         RowBox[{"1", "+", "u"}]]}], ",", "\[IndentingNewLine]", 
       RowBox[{"-", 
        RowBox[{"Log", "[", 
         RowBox[{"RandomReal", "[", "]"}], "]"}]}], " ", 
       RowBox[{"(*", " ", 
        RowBox[{"sample", " ", "m1", 
         RowBox[{"(", "s", ")"}]}], " ", "*)"}], "\[IndentingNewLine]", ",", 
       "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"xi1", "<", 
          RowBox[{
           FractionBox["u", 
            RowBox[{"1", "+", "u"}]], "+", 
           RowBox[{"(", 
            RowBox[{"1", "-", "u"}], ")"}]}]}], ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"-", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"-", "1"}], "+", 
             SuperscriptBox["u", "2"]}], ")"}]}], " ", 
          SuperscriptBox[
           RowBox[{"InverseErf", "[", 
            RowBox[{"RandomReal", "[", "]"}], "]"}], "2"]}], " ", 
         RowBox[{"(*", " ", 
          RowBox[{"sample", " ", "m3"}], " ", "*)"}], "\[IndentingNewLine]", 
         ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"-", 
           RowBox[{"Log", "[", 
            RowBox[{"RandomReal", "[", "]"}], "]"}]}], "-", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"-", "1"}], "+", 
             SuperscriptBox["u", "2"]}], ")"}], " ", 
           SuperscriptBox[
            RowBox[{"InverseErf", "[", 
             RowBox[{"RandomReal", "[", "]"}], "]"}], "2"]}]}]}], "  ", 
        RowBox[{"(*", " ", 
         RowBox[{
          RowBox[{"sample", " ", "m2"}], " ", "-", " ", 
          RowBox[{"by", " ", "convolution"}]}], " ", "*)"}], 
        "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", "]"}]}]}], 
